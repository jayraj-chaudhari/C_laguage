//Fibonacci series
//Jayraj Chaudhari
#include<stdio.h>
int main()
{
    int n1=0,n2=1,terms,a=1,sum;
    printf("Enter the no of terms for fibonacci series");
    scanf("%d",&terms);
    printf("0 1 ");
    while(a<=terms)
    {
        sum=n1+n2;
        n1=n2;
        n2=sum;
       
        a++;
        printf("%d ",n2);
    }
}
