//number is prime or not
#include<stdio.h>
int main()
{
    int num,a,flag=0;
    printf("Enter the number");
    scanf("%d",&num);
    for(a=2;a<=num/2;a++)
    {
        if(num%a==0)
        {
            flag=1;
        }
    }
    if(flag==0)
    printf("%d is prime",num);
    else
    printf("%d is not prime",num);
}
