//Program to read three numbers and print maximum using conditional oprator7
//Jayraj Chaudhari
#include<stdio.h>
int main()
{
     int a, b, c,d,e,f;
    printf("Enter three numbers:");
    scanf("%d%d%d", &a, &b, &c);
    d=a>b?a:b;
    e=b>c?b:c;
    f=e>d?e:d;
    printf("%d is the greatest.",f);
    return 0;
}
