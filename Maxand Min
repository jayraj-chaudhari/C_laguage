//Program to find max and min from given N no.
//Jayraj Chaudhari
#include<stdio.h>
int main()
{
    int n,num,max,min,i,num1;
    printf("How many numbers you want to enter:");
    scanf("%d",&n);
    printf("Enter 1st number:");
    scanf("%d",&num1);
    max=num1;
    min=num1;
    for(i=2;i<=n;i++)
    {
        printf("Enter %d number:",i);
        scanf("%d",&num);
        if(num>max)
        {
            max=num;
        }
        else if (num<min)
        {
           min=num;
        }
    }
    printf("Maximum = %d\nmin = %d",max,min);
    return 0;
}
