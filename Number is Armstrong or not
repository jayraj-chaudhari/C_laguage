//Wether number is armstrong or not
//Jayraj Chaudhari
#include<stdio.h>
int main()
{
    int num,a,q,sumcube=0;
    printf("Enter the number:");
    scanf("%d",&num);
    a=num;
    for(num;num>0;num=num/10)
    {
        q=num%10;
        sumcube=sumcube+q*q*q;
    }
    if(sumcube==a)
    printf("No is Armstrong");
    else
    printf("Entered no is not armstrong");
    return 0;
}
