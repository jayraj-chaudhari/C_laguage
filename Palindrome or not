//Program to ckeck if the number is palindrome
//Jayraj Chaudhari
#include<stdio.h>
int main()
{
    int num,num2,q,a;
    printf("Enter the number");
    scanf("%d",&num);
    num2=0;
    a=num;
    for(num;num>0;num=num/10)
    {
        q=num%10;
        num2=num2*10 +q;
    }
   if(a==num2)
   {
    printf("Given no is pallandrome.");
   }
   else
   {
    printf("Entered no is not a pallandrome");
   }
    return 0;
    
}
