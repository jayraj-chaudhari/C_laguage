//Program to find max and second min from given N no.
//Jayraj Chaudhari
#include<stdio.h>
int main()
{
    int n,num,max,max2,i,num1;
    printf("How many numbers you want to enter:");
    scanf("%d",&n);
    printf("Enter 1st number:");
    scanf("%d",&num1);
    max=num1;
    max2=num1;
    for(i=2;i<=n;i++)
    {
        printf("Enter %d number:",i);
        scanf("%d",&num);
        if(num>max)
        {
            max2=max;
            max=num;
        }
 }
    printf("Maximum = %d\nSecond maximum = %d",max,max2);
    return 0;
}
