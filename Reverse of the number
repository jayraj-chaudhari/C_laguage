//Program to reverse a number 
//Jayraj Chaudhari
#include<stdio.h>
int main()
{
    int num,num2,q;
    num2=0;
    printf("Enter the number");
    scanf("%d",&num);
    for(num;num>0;num=num/10)
    {
        q=num%10;
        num2=num2*10 +q;
    }
    printf("Reverse of the given no is %d",num2);
    return 0;
}
