//Convert tamrature from farenheit to celcius
//Jayraj Chaudhari


#include<stdio.h>
int main()
{
    float farenheit,celcius;
    printf("Entre the tamprature in frenheit:");
    scanf("%f",&farenheit);
    celcius=(farenheit-32)*5/9;

    printf("The value of tamprature in celcius is %f",celcius);
    
    return 0;

}
